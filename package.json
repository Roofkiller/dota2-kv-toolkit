{
    "name": "dota-2-kv-toolkit",
    "displayName": "Dota 2 KV Toolkit",
    "description": "",
    "version": "1.1.1",
    "publisher": "nicholasgao",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.mergeKv",
        "onCommand:extension.autoMergeKv"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.mergeKv",
                "title": "Merge KVs"
            },
            {
                "command": "extension.autoMergeKv",
                "title": "Auto merge KVs"
            }
        ],
        "keybindings": [
            {
                "command": "extension.mergeKv",
                "key": "shift+alt+m"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "vscode": "^1.1.9",
        "@types/node": "^10.3.1",
        "@types/mocha": "^5.2.1"
    }
}
